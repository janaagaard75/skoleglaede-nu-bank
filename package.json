{
  "name": "skoleglaede-nu-bank",
  "version": "1.2.3",
  "description": "Virtual wallet for a game about private economy created by Skolegl√¶de.nu.",
  "author": "Jan Aagaard <jan@aagaard.net>",
  "private": true,
  "license": "MIT",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "build": "npx eas-cli@latest build --platform all",
    "build-android": "npx eas-cli@latest build --platform android",
    "build-ios": "npx eas-cli@latest build --platform ios",
    "check-types": "tsc",
    "check-lint": "eslint --format unix --max-warnings 0",
    "optimize-assets": "expo optimize",
    "publish": "expo publish",
    "start": "yarn install && expo start --ios",
    "update-dependencies": "yarn upgrade && npx syncyarnlock -k -s && yarn install",
    "validate": "yarn install --frozen-lockfile && yarn run check-types && yarn run check-lint"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22"
  },
  "dependencies": {
    "@babel/core": "^7.24.0",
    "@expo/vector-icons": "^14.0.3",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@types/react": "~18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "babel-preset-expo": "~11.0.0",
    "eslint": "^9.30.0",
    "eslint-formatter-unix": "^8.40.0",
    "eslint-plugin-filename-export": "^1.0.4",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "expo": "~51.0.39",
    "expo-camera": "~15.0.16",
    "expo-secure-store": "~13.0.2",
    "expo-updates": "~0.25.27",
    "prettier": "^3.3.3",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "tslib": "^2.5.0",
    "typescript": "~5.3.3",
    "typescript-eslint": "^8.35.0"
  },
  "packageManager": "yarn@4.9.2"
}
