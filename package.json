{
  "name": "skoleglaede-nu-bank",
  "version": "1.2.3",
  "description": "Virtual wallet for a game about private economy created by Skolegl√¶de.nu.",
  "author": "Jan Aagaard <jan@aagaard.net>",
  "private": true,
  "license": "MIT",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "build": "eas build --platform all",
    "build-ios": "eas build --platform ios",
    "check-types": "tsc",
    "lint": "eslint --format unix \"**/*.ts\" \"**/*.tsx\"",
    "optimize-assets": "expo optimize",
    "publish": "expo publish",
    "start": "yarn install && expo start --ios",
    "update-dependencies": "yarn upgrade && npx syncyarnlock -k -s && yarn install",
    "validate": "yarn install --frozen-lockfile && yarn run check-types && yarn run lint"
  },
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
